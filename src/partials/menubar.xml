<MenuBar>
	<Menu name="Synth">
		<Menu name="About Synth" shell="win -a"/>
		<Menu type="divider"/>
		<Menu name="Quit Synth" shell="win -c"/>
	</Menu>
	<Menu name="Help">
		<Menu name="Send Synth Feedback"/>
		<Menu type="divider"/>
		<Menu name="Synth Help"/>
		<Menu type="divider"/>
		<Menu name="License"/>
	</Menu>
	<Menu for="score-board">
		<Menu name="About Synth" shell="win -a"/>
		<Menu type="divider"/>
		<Menu name="Reverb">
			<Menu name="Off" click="toggle-reverb" arg="" check-group="midi-reverb" is-checked="1"/>
			<Menu type="divider"/>
			<Menu name="Large Hall" click="toggle-reverb" arg="~/js/ir-large-hall.ogg" check-group="midi-reverb"/>
		</Menu>
		<Menu type="divider"/>
		<Menu type="caption" name="Volume:"/>
		<Menu type="slider" min="0" max="100" value="100" change="midi-volume"/>
		<Menu type="divider"/>
		<Menu type="caption" name="Seek:"/>
		<Menu type="slider" min="0" max="100" value="0" change="midi-seek"/>
	</Menu>
</MenuBar>